{
  "extends": ["stylelint-config-sass-guidelines", "stylelint-config-prettier"],
  "plugins": ["stylelint-prettier"],
  "rules": {
    "prettier/prettier": true,
    "max-nesting-depth": [5],
    "selector-max-compound-selectors": [5],
    "rule-empty-line-before": [
      "always",
      {
        "ignore": ["after-comment"]
      }
    ]
  }
}
